   "source": [
    "리스트 변수로 스택을 다루는 pop, push 기능 구현하기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "offensive-nickname",
   "metadata": {},
   "outputs": [],
   "source": [
    "arr = []\n",
    "\n",
    "def push(i):\n",
    "    arr.append(i)\n",
    "\n",
    "def pop():\n",
    "    data = arr[-1]\n",
    "    del arr[-1]\n",
    "    return data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "urban-ending",
   "metadata": {},
